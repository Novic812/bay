# must run a single time to avoid repeated downloading of setup.ini

k=(
  git     # 53 occurances
  wget    # 17 occurances
  make    # 12 occurances
  curl    # 9 occurances
  unzip   # 3 occurances
  zip     # 3 occurances
  openssh # git push
)

IFS=, read k <<< "${k[*]}"
setup -nq -s http://box-soft.com -l $PUBLIC -P $k
