#!/bin/sh
# This file will install Cygwin packages, similar to .brew

# must include HOMEDRIVE
setup -nq -l $HOMEDRIVE/cygwin/usr/local/bin -s http://box-soft.com

# PolarSSL
setup -nqP wget

# RtmpDump
setup -nqP git
setup -nqP make
setup -nqP mingw64-i686-gcc-core

# git push
setup -nqP openssh

# git send-email
setup -nqP msmtp

# php and google
setup -nqP unzip
