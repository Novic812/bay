<RDF:RDF xmlns:NC="http://home.netscape.com/NC-rdf#"
    xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <RDF:Description RDF:about="chrome://browser/content/browser.xul">

    <!-- bookmarks toolbar -->
    <NC:persist RDF:resource="chrome://browser/content/browser.xul#PersonalToolbar"
      collapsed="true" currentset="__empty"/>

    <!-- status bar -->
    <NC:persist RDF:resource="chrome://browser/content/browser.xul#addon-bar"
      currentset="status4evar-status-widget" collapsed="false"/>

    <!-- need bookmarks-menu-button-container to trick spacing for hds -->
    <NC:persist RDF:resource="chrome://browser/content/browser.xul#nav-bar"
      currentset="unified-back-forward-button,history-button,abp-toolbarbutton,firebug-button,spacer,widget:jid0-HFFmJoceGjTSKDBEWPpzfX9By7I@jetpack-hds-link-detector,bookmarks-menu-button-container,spacer,personal-bookmarks"/>

    <!-- menu bar -->
    <NC:persist RDF:resource="chrome://browser/content/browser.xul#toolbar-menubar"
      currentset="menubar-items,urlbar-container" autohide="false"/>

  </RDF:Description>
  <RDF:Description RDF:about="chrome://browser/content/history/history-panel.xul">

    <!-- history panel -->
    <NC:persist RDF:resource="chrome://browser/content/history/history-panel.xul#viewButton"
      selectedsort="lastvisited"/>

  </RDF:Description>
</RDF:RDF>
