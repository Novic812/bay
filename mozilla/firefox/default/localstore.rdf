<RDF xmlns:NC="http://home.netscape.com/NC-rdf#"
  xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#">

  <Description about="chrome://browser/content/browser.xul">
    <NC:persist resource="#PersonalToolbar"/>
    <NC:persist resource="#addon-bar"/>
    <NC:persist resource="#nav-bar"/>
    <NC:persist resource="#toolbar-menubar"/>
    <NC:persist resource="#sidebar-box"/>
  </Description>

  <Description about="chrome://browser/content/history/history-panel.xul">
    <NC:persist resource="#viewButton"/>
  </Description>

  <Description about="chrome://dta/content/dta/manager.xul">
    <NC:persist resource="#DownThemAll"/>
    <NC:persist resource="#dim"/>
    <NC:persist resource="#per"/>
    <NC:persist resource="#speed"/>
  </Description>

  <!-- bookmarks toolbar -->
  <Description ID="PersonalToolbar" currentset="__empty" collapsed="true">

  <!-- status bar -->
  <Description ID="addon-bar"
  currentset="status4evar-status-widget" collapsed="false">

  <!-- nav bar -->
  <Description ID="nav-bar"
  currentset="unified-back-forward-button,downloads-button,tabview-button,abp-toolbarbutton,personal-bookmarks">

  <!-- menu bar -->
  <Description ID="toolbar-menubar" currentset="menubar-items,urlbar-container"
  autohide="false">

  <!-- history width -->
  <Description ID="sidebar-box" width="300">

  <!-- history button -->
  <Description ID="viewButton" selectedsort="lastvisited">

  <!-- window -->
  <Description ID="DownThemAll" height="400" width="800">

  <!-- Size -->
  <Description ID="dim" width="150">

  <!-- Percent -->
  <Description ID="per" width="70">

  <!-- Speed -->
  <Description ID="speed" width="100">

</RDF>
